pipeline {
  agent none
  stages {
    stage('Build') {
      steps {
        echo 'Build is Complete'
      }
    }
    stage('Test') {
      steps {
        echo 'Test is complete'
      }
    }
    stage('Deploy') {
      steps {
        echo 'App is deployed!'
      }
    }
    stage('Maintain - Pipeline Complete') {
      steps {
        echo 'Pipeline is complete.  App maintenance and upgrading may begin.'
      }
    }
  }
}